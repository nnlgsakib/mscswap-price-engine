{
  "version": 2,
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "coins/**"
        ],
        "excludeFiles": [
          "src/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.ts"
    }
  ],
  "env": {
    "PORT": "3000"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/index.ts"
    }
  ]
}
